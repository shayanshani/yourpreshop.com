@{
    ViewBag.Title = "About Us";
}

<!--breadcrumbs area start-->
<div class="breadcrumbs_area commun_bread">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="breadcrumb_content">
                    <h3>about us</h3>
                    <ul>
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li><i class="fa fa-angle-right"></i></li>
                        <li>about us</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
<!--breadcrumbs area end-->
<!--about section area -->
<div class="about_section">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-md-12">
                <div class="about_content">
                    <h1>Welcome To PreShop!</h1>
                    <h4 style="color:#333">PRESHOP</h4>
                    <p>Preshop is an online shopping platform which provides solution to your daily shopping needs with ease, reliability and convenience.</p>
                    <h4 style="color:#333">VISION</h4>
                    <p>To be a leading online shopping marketplace, which fulfills the needs of customers and provides an online solution to daily operations of shopkeepers, distributors and manufacturers.</p>
                    <h4 style="color:#333">MISSION</h4>
                    <p>Refashion the trends of online shopping and stay ahead of the game.</p>
                    <div class="view__work">
                        <a href="#">view work </a>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-12">
                <div class="about_thumb">
                    <img src="~/assets/img/ship/about1.jpg" alt="">
                </div>
            </div>
        </div>
    </div>
</div>
<!--about section end-->
<!--counterup area -->
<div class="counterup_section">
    <div class="container ">
        <div class="progressbar_inner">
            <h2>OUR VALUES</h2>
        </div>
        <div class="row">
            <div class="col-lg-3 col-md-6 col-sm-6">
                <div class="single_counterup">
                    <div class="counter_info">
                        @*<h2 class="counter_number">Leadership</h2>*@
                        <h4 style="color:#333">LEADERSHIP</h4>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6">
                <div class="single_counterup count-two">
                    <div class="counter_info">
                        @*<h2 class="counter_number">8080</h2>*@
                        <h4 style="color:#333">ENTERPRENUERSHIP</h4>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6">
                <div class="single_counterup">
                    <div class="counter_info">
                        @*<h2 class="counter_number">2150</h2>*@
                        <h4 style="color:#333">COMITMENT</h4>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6">
                <div class="single_counterup count-two">
                    <div class="counter_info">
                        @*<h2 class="counter_number">2170</h2>*@
                        <h4 style="color:#333">RISK TAKING</h4>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-3 col-md-6 col-sm-6">
                <div class="single_counterup">
                    <div class="counter_info">
                        @*<h2 class="counter_number">2170</h2>*@
                        <h4 style="color:#333">DIVERSITY</h4>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6">
                <div class="single_counterup count-two">
                    <div class="counter_info">
                        @*<h2 class="counter_number">8080</h2>*@
                        <h4 style="color:#333">AGILITY</h4>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6">
                <div class="single_counterup">
                    <div class="counter_info">
                        @*<h2 class="counter_number">2150</h2>*@
                        <h4 style="color:#333">INNOVATION</h4>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6">
                <div class="single_counterup count-two">
                    <div class="counter_info">
                        @*<h2 class="counter_number">2170</h2>*@
                        <h4>TEAM WORK</h4>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--counterup end-->
<!--about progress bar -->
<div class="about_progressbar">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6 col-md-6">
                <div class="progressbar_inner">
                    <h2>WHAT WE STRIVE FOR</h2>
                    <div class="progress_skill">
                        <div class="progress">
                            <div class="progress-bar about_prog wow fadeInLeft" data-wow-duration="0.8s" data-wow-delay=".3s" role="progressbar" style="width: 100%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
                                <span class="progress_persent">YOUR SATISFACTION</span>
                            </div>
                        </div>
                        <span class="progress_discount">1</span>
                    </div>
                    <div class="progress_skill">
                        <div class="progress">
                            <div class="progress-bar about_prog wow fadeInLeft" data-wow-duration="0.8s" data-wow-delay=".5s" role="progressbar" style="width: 100%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
                                <span class="progress_persent">YOUR RELIABILITY</span>
                            </div>
                        </div>
                        <span class="progress_discount">2</span>
                    </div>
                    <div class="progress_skill">
                        <div class="progress">
                            <div class="progress-bar about_prog wow fadeInLeft" data-wow-duration="0.8s" data-wow-delay=".7s" role="progressbar" style="width: 100%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
                                <span class="progress_persent">YOUR TRUST</span>
                            </div>
                        </div>
                        <span class="progress_discount">3</span>
                    </div>
                    <div class="progress_skill">
                        <div class="progress">
                            <div class="progress-bar about_prog wow fadeInLeft" data-wow-duration="0.8s" data-wow-delay=".7s" role="progressbar" style="width: 100%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
                                <span class="progress_persent">YOUR CONVENIENCE</span>
                            </div>
                        </div>
                        <span class="progress_discount">4</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-6">
                <div class="about__img">
                    <img src="assets/img/ship/about3.jpg" alt="">
                </div>
            </div>
        </div>
    </div>
</div>
<!--about progress bar end -->