@model PreShop.FrontEnd.Helpers.Products.ProductsHelper

@{
    var products = Model.GetProductsHomePage();
    if (products.Count > 0)
    {
        var r = new Random();
        for (int i = 0; i <= products.Count; i++)
        {
            var p = products[r.Next(0, products.Count)];
            <div class="col-lg-3">
                @Html.Partial("_Product",p)
            </div>
            products.Remove(p);
        }
    }
}
